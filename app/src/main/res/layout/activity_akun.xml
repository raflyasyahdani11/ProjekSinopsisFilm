<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A1000000">

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="122dp"
        android:layout_height="25dp"
        android:layout_x="183dp"
        android:layout_y="357dp"
        android:ems="10"
        android:inputType="textPassword" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="57dp"
        android:layout_y="253dp"
        android:text="Nama"
        android:textSize="18sp" android:textColor="@color/white">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="60dp"
        android:layout_y="308dp"
        android:text="Email"
        android:textSize="18sp" android:textColor="@color/white">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="61dp"
        android:layout_y="359dp"
        android:text="Password"
        android:textSize="18sp" android:textColor="@color/white"/>

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="126dp"
        android:layout_height="22dp"
        android:layout_x="184dp"
        android:layout_y="301dp"
        android:ems="10"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="132dp"
        android:layout_height="23dp"
        android:layout_x="182dp"
        android:layout_y="250dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="235dp"
        android:layout_y="438dp"
        android:text="Back" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="148dp"
        android:layout_height="155dp"
        android:layout_x="124dp"
        android:layout_y="39dp"
        app:srcCompat="@drawable/ava1" />

</AbsoluteLayout>